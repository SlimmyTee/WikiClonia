{
    "name": "bemit",
    "version": "1.0.0",
    "description": "Template project with BEM and ITCSS methodology",
    "main": "gulpfile.js",
    "scripts": {
        "start": "webpack-dev-server --config ./webpack.config.js --mode development --hot --inline",
        "build": "webpack --config ./webpack.config.js --mode production",
        "webpack": "webpack --profile --json > stats.json",
        "test": "jest",
        "lint": "npx eslint --ignore-pattern '/**/test.js' 'src/**/*.js'",
        "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
    },
    "jest": {
        "verbose": false,
        "transform": {
            "^.+\\.svg$": "<rootDir>/svgTransform.js",
            "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest"
        },
        "moduleNameMapper": {
            "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
            "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
        },
        "coverageDirectory": "./coverage/",
        "collectCoverage": true
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint && npm test",
            "pre-push": "npm run lint && npm test"
        }
    },
    "keywords": [
        "BEM",
        "ITCSS",
        "SASS"
    ],
    "author": "Nam L. Hoang",
    "license": "ISC",
